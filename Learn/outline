struct dyn_array {
    data: void*,
    size: u32,
    capacity: u32
};

dyn_array DynArray = dyn_array_new(10);

a : [10] u32; // fixed size array
a : [..] i32; // dynamic array

str : [..] u8 = string("fdfdf");
