decl 
    int swap(int *x, int *y);
enddecl

int swap(int *x, int *y)
{
    decl int tmp; enddecl
    begin 
        tmp = *x;
        *x = *y;
        *y = tmp;
    return 0;
    end
}

int main()
{
    decl 
        int a, b; 
    enddecl
    begin
        a = 10;
        b = 20;
        write(a);
        write(b);
        swap(&a, &b);
        write(a);
        write(b);
        return 0;
    end
}