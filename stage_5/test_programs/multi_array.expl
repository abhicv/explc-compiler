decl int arr[10][10]; enddecl

int main()
{
    decl int n, m; enddecl
    begin
        n = 0;
        while(n < 10) do
            m = 0;
            while(m < 10) do
                arr[n][m] = n * m;
                m = m + 1;
            endwhile;
            n = n + 1;
        endwhile;

        n = 0;
        while(n < 10) do
            m = 0;
            while(m < 10) do
                write(arr[n][m]);
                m = m + 1;
            endwhile;
            n = n + 1;
        endwhile;

        return 0;
    end
}